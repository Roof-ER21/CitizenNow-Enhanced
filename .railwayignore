*.spec.ts
*.spec.tsx
__tests__/
__mocks__/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Build artifacts (will be rebuilt on Railway)
dist/
build/
.next/
out/

# Documentation (not needed in production)
*.md
!README.md
docs/

# Git
.git/
.gitignore
.gitattributes

# CI/CD
.github/
.gitlab-ci.yml
.travis.yml

# Native build directories
ios/
android/
*.keystore

# Deployment scripts (not needed on Railway)
build-android.sh
